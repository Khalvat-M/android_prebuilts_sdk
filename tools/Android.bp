cc_prebuilt_shared_library {
    name: "libLLVM",
    host_supported: true,
    target: {
        linux_x86_64: {
            srcs: ["linux/lib64/libLLVM.so"],
        },
        darwin_x86_64: {
            srcs: ["darwin/lib64/libLLVM.dylib"],
